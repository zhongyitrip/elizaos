    "evt": "^2.5.9",
    "lucide-react": "^0.562.0",
    "react": "^19.1.0",
    "react-aiwriter": "^1.0.0",
    "react-dom": "^19.1.0",
    "react-force-graph": "^1.47.6",
    "react-force-graph-2d": "^1.27.1",
    "react-joyride": "^2.9.3",
