{
  "name": "@elizaos/client",
  "version": "1.7.3-beta.1",
  "description": "Web client interface for ElizaOS agents",
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/eliza.git",
    "directory": "packages/client"
  },
  "license": "MIT",
  "keywords": [
    "elizaos",
    "ai",
    "agent",
    "client",
    "react",
    "web",
    "chat",
    "interface"
  ],
  "homepage": "https://elizaos.ai",
  "bugs": {
    "url": "https://github.com/elizaos/eliza/issues"
  },
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist",
    "public",
    "src",
    "index.html",
    "vite.config.ts",
    "tsconfig.json",
    "postcss.config.js",
    "tailwind.config.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "start:client": "vite",
    "dev:client": "vite --host 0.0.0.0 --port 5173",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src/ && prettier --write ./src",
    "lint:check": "eslint src/",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json *.tsbuildinfo",
    "test": "bun test",
    "test:all": "./scripts/test-all.sh",
    "test:quick": "./scripts/test-quick.sh",
    "test:unit": "./scripts/run-bun-tests.sh --coverage",
    "test:unit:watch": "./scripts/run-bun-tests.sh --watch",
    "test:component": "cypress run --component",
    "test:e2e": "cypress run --e2e",
    "test:e2e:server": "./scripts/test-e2e-server.sh",
    "test:e2e:server:auth": "./scripts/test-e2e-server-auth.sh",
    "cypress:open": "cypress open",
    "type-check": "cd cypress && bunx tsc --noEmit --project tsconfig.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@elizaos/api-client": "workspace:*",
    "@elizaos/core": "workspace:*",
    "@evilmartians/agent-prism-data": "^0.0.9",
