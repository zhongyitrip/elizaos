{
    "name": "@elizaos/plugin-airdrop",
    "version": "0.1.0",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "description": "ElizaOS plugin for managing 30,000 EOA addresses for airdrop hunting",
    "repository": {
        "type": "git",
        "url": "https://github.com/your-username/elizaos"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "characters"
    ],
    "dependencies": {
        "@elizaos/core": "workspace:*",
        "@supabase/supabase-js": "^2.90.1",
        "ethers": "^6.16.0"
    },
    "devDependencies": {
        "@elizaos/config": "workspace:*",
        "@types/node": "^24.10.1",
        "typescript": "^5.9.3"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rm -rf dist .turbo node_modules",
        "test": "bun test __tests__",
        "test:watch": "bun test __tests__ --watch",
        "generate-eoa": "bun run scripts/generate-eoa-addresses.ts",
        "process-airdrops": "bun run scripts/process-airdrops.ts"
    }
}